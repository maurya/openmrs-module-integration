<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.openmrs.module.integration.CategoryOptionToOption" table="dhis_categoryoption_option">
		<id name="categoryOptionToOptionId" type="int" column="id"
			unsaved-value="0">
			<generator class="native" />
		</id>
		<discriminator column="id" insert="false" />
		<many-to-one name="categoryOption" class="org.openmrs.module.integration.CategoryOption"
			not-null="true">
			<column name="categoryoption_id" />
		</many-to-one>
		<many-to-one name="option" class="org.openmrs.module.integration.Option"
			not-null="true">
			<column name="option_id" />
		</many-to-one>
</class>
</hibernate-mapping>