<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.openmrs.module.integration.OptionSet" table="optionset">
		<id name="optionSetId" type="long" column="id"
			unsaved-value="0">
			<generator class="native" />
		</id>
		<discriminator column="id" insert="false" />

		<property name="uuid" type="java.lang.String" column="uuid"
			length="38" unique="true" />
			
			

		<property name="optionSetName" type="java.lang.String" column="optionset_name"
			not-null="true" length="255" />
			
			<property name="optionSetCode" type="java.lang.String" column="optionset_code"
			not-null="true" length="255" />
			
			<property name="optionSetValueName" type="java.lang.String" column="optionset_value_name"
			not-null="true" length="255" />
			
			<property name="optionSetValueCode" type="java.lang.String" column="optionset_value_code"
			not-null="true" length="255" />
			
			<property name="mappedObjectType" type="java.lang.String" column="mappedobject_type" 
			not-null="false" length="255"/>
			
			<property name="mappedObjectId" type="java.lang.String" column="mappedobject_id"
			 not-null="false" length="255"/>
			
			
			<property name="serverId" type="long" column="server_id"
			not-null="true" />
		<many-to-one name="id" class="org.openmrs.module.integration.IntegrationServer"
			not-null="true">
			<column name="serverId" />
		</many-to-one>
		
	</class>
</hibernate-mapping>