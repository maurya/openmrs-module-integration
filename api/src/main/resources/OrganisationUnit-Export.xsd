<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:jxb="http://java.sun.com/xml/ns/jaxb" jxb:version="2.0" 
	targetNamespace="http://www.dhis.org/schemas/DXF/2.0/" 
	xmlns:tns="http://www.dhis.org/schemas/DXF/2.0/" elementFormDefault="qualified">
	<xsd:complexType name="OrgUnitType">
		<xsd:attribute type="xsd:string" name="code" use="required" />
		<xsd:attribute type="xsd:string" name="name" use="required" />
		<xsd:attribute type="xsd:dateTime" name="created" use="optional" />
		<xsd:attribute type="xsd:dateTime" name="lastUpdated" use="optional" />
		<xsd:attribute type="xsd:string" name="shortName" use="optional" />
		<xsd:attribute type="xsd:byte" name="level" use="optional" />
		<xsd:attribute type="xsd:anyURI" name="href" use="optional" />
		<xsd:attribute type="xsd:string" name="id" use="required" />
	</xsd:complexType>

	<xsd:element name="metaData">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="organisationUnits">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="organisationUnit" maxOccurs="unbounded" minOccurs="0">
								<xsd:complexType>
									<xsd:complexContent>
										<xsd:extension base="tns:OrgUnitType">
											<xsd:sequence>
												<xsd:element type="tns:OrgUnitType" name="parent" minOccurs="0" />
												<xsd:element type="xsd:date" name="openingDate" minOccurs="0" />
												<xsd:element type="xsd:boolean" name="active" minOccurs="0" />
											</xsd:sequence>
										</xsd:extension>
									</xsd:complexContent>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
