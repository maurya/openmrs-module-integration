<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.openmrs.module.integration.DataValueTemplate" table="dhis_datavaluetemplate">
		<id name="dataValueTemplateId" type="long" column="id"
			unsaved-value="0">
			<generator class="native" />
		</id>
		<discriminator column="id" insert="false" />

		<property name="uuid" type="java.lang.String" column="uuid"
			length="38" unique="true" />
			
				<many-to-one name="dataElementId" class="org.openmrs.module.integration.DataElement"
			not-null="true">
			<column name="dataelementId_id" />
		</many-to-one>

				<many-to-one name="categoryComboId" class="org.openmrs.module.integration.CategoryCombo"
			not-null="true">
			<column name="categorycombo_id" />
		</many-to-one>
		
				<many-to-one name="reportTemplateId" class="org.openmrs.module.integration.ReportTemplate"
			not-null="true">
			<column name="reporttemplate_id" />
		</many-to-one>
			
			
 			<property name="lastUpdated" type="java.util.Date" column="last_updated"
			length="19" />
		
	</class>
</hibernate-mapping>